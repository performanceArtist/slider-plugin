{
    "compilerOptions": {
    "typeRoots": ["./node_modules/@types"],
    "strict": true,
    "strictPropertyInitialization": false,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
    },
    "allowJs": true,
    "exclude": [
      "./node_modules/**/*"
    ]
}
